<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cl_member_container"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="25dp"
    android:layout_marginEnd="19dp">

    <ImageView
        android:id="@+id/iv_member_container"
        android:layout_width="64dp"
        android:layout_height="64dp"
        android:src="@drawable/background_member_orange"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageView
        android:id="@+id/iv_member"
        android:layout_width="36.2dp"
        android:layout_height="36.2dp"
        android:src="@drawable/group_390"
        app:layout_constraintBottom_toBottomOf="@id/iv_member_container"
        app:layout_constraintEnd_toEndOf="@id/iv_member_container"
        app:layout_constraintStart_toStartOf="@id/iv_member_container"
        app:layout_constraintTop_toTopOf="@id/iv_member_container" />


    <TextView
        android:id="@+id/tv_member_nickname"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="10dp"
        android:fontFamily="@font/roboto_medium"
        android:textColor="@color/black"
        android:textSize="10sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/iv_member_container"
        tools:text="불뚝" />

</androidx.constraintlayout.widget.ConstraintLayout>